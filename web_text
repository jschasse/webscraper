  <script id="sp_cookie">
   function getSpCookie(name) {
    var cookieStart = document.cookie.indexOf(name + "=");
    if (cookieStart === -1) return null;
    
    var cookieEnd = document.cookie.indexOf(";", cookieStart);
    if (cookieEnd === -1) cookieEnd = document.cookie.length;
    
    return document.cookie.substring(cookieStart + name.length + 1, cookieEnd);
}

function upsertSpCookie(cookieName, testId, testBucket) {
    const expireTime = new Date();
    const oneDayInMs = 24 * 60 * 60 * 1000;

    expireTime.setTime(expireTime.getTime() + oneDayInMs);
    const expires = expireTime.toUTCString();

    const spCookie = getSpCookie(cookieName);
    
    if (!spCookie) {
        // No current cookie, so set one.
        document.cookie = cookieName + "=" + testId + testBucket + "; expires=" + expires + "; path=/";
    } else if (spCookie.indexOf(testId) === -1) {
        // Cookie exists, but without this test. Add it.
        document.cookie = cookieName + "=" + testId + testBucket + "|" + spCookie + "; expires=" + expires + "; path=/";
    } else {
        // Cookie exists, refresh the expiry.
        document.cookie = cookieName + "=" + spCookie + "; expires=" + expires + "; path=/";
    }
}

upsertSpCookie("sp-cdn-bucket", "res-5450-", "control");
  </script>
